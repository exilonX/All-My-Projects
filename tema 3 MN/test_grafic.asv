function test_grafic(eps)
x=linspace(-pi,pi,1001);
for j=1:1001
			fx(j)=exp(3*cos(x(j)))/(2*pi*besseli(0,3));
end
    subplot(1,2,1) , plot(x,fx,'r');
    
    hold on;
    [N,fa]=eval_interpolator_c(2,eps);
    if N~=inf
    plot(x,fa,'g');
    end
    
    
    
end
    
    
    
    